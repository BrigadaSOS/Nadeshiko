meta {
  name: List episodes for a media
  type: http
  seq: 15
}

get {
  url: {{baseUrl}}/v1/media/:mediaId/episodes
  body: none
  auth: apikey
}

headers {
  X-API-Key: {{apiKey}}
}

params:query {
  mediaId:
  size:
  cursor:
}

auth:apikey {
  key: X-API-Key
  value: {{apiKey}}
  placement: header
}

settings {
  encodeUrl: true
  timeout: 0
}
