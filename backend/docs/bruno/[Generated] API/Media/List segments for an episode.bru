meta {
  name: List segments for an episode
  type: http
  seq: 20
}

get {
  url: {{baseUrl}}/v1/media/:mediaId/episodes/:episodeNumber/segments
  body: none
  auth: apikey
}

headers {
  X-API-Key: {{apiKey}}
}

params:query {
  mediaId:
  episodeNumber:
  size:
  cursor:
}

auth:apikey {
  key: X-API-Key
  value: {{apiKey}}
  placement: header
}

settings {
  encodeUrl: true
  timeout: 0
}
