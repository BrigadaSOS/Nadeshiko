}

body:json {
  {
    "episodeNumber": 0,
    "anilistEpisodeId": 0,
    "titleEnglish": "",
    "titleRomaji": "",
    "titleJapanese": "",
    "description": "",
    "airedAt": "",
    "lengthSeconds": 0,
    "thumbnailUrl": ""
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
