meta {
  name: Logout the user
  type: http
  seq: 11
}

post {
  url: {{baseUrl}}/v1/auth/logout
  body: none
  auth: none
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const jwtToken = bru.getEnvVar("jwtToken");
  if (jwtToken) {
    req.setHeader("Cookie", "access_token=" + jwtToken);
  }
}
