meta {
  name: (OAuth) Login with Discord
  type: http
  seq: 7
}

post {
  url: {{baseUrl}}/v1/auth/discord
  body: json
  auth: inherit
}

headers {
  Referer: {{frontendUrl}}
}

body:json {
  {
    "code": "1234567890"
  }
}

settings {
  encodeUrl: true
  timeout: 0
}

script:post-response {
  if (res.getStatus() >= 200 && res.getStatus() < 300) {
      const json = res.getBody();
      if (json.token) {
          bru.setEnvVar("jwtToken", json.token);
      }
  }
}
