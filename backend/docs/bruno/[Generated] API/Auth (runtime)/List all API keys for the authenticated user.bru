meta {
  name: List all API keys for the authenticated user
  type: http
  seq: 41
}

get {
  url: {{baseUrl}}/api/auth/api-key/list
  body: none
  auth: none
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const sessionToken = bru.getEnvVar("sessionToken");
  if (sessionToken) {
    req.setHeader("Cookie", "nadeshiko.session_token=" + sessionToken);
  }
}
