meta {
  name: Get the current session
  type: http
  seq: 45
}

get {
  url: {{baseUrl}}/api/auth/get-session
  body: none
  auth: none
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const sessionToken = bru.getEnvVar("sessionToken");
  if (sessionToken) {
    req.setHeader("Cookie", "nadeshiko.session_token=" + sessionToken);
  }
}
