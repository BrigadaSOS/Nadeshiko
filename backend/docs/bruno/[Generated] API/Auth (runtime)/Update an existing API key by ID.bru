meta {
  name: Update an existing API key by ID
  type: http
  seq: 42
}

post {
  url: {{baseUrl}}/api/auth/api-key/update
  body: none
  auth: none
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const sessionToken = bru.getEnvVar("sessionToken");
  if (sessionToken) {
    req.setHeader("Cookie", "nadeshiko.session_token=" + sessionToken);
  }
}
