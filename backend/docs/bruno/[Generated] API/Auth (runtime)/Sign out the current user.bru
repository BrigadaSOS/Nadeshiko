meta {
  name: Sign out the current user
  type: http
  seq: 52
}

post {
  url: {{baseUrl}}/api/auth/sign-out
  body: none
  auth: none
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const sessionToken = bru.getEnvVar("sessionToken");
  if (sessionToken) {
    req.setHeader("Cookie", "nadeshiko.session_token=" + sessionToken);
  }
}
