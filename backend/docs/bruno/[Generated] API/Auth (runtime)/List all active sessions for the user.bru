meta {
  name: List all active sessions for the user
  type: http
  seq: 46
}

get {
  url: {{baseUrl}}/api/auth/list-sessions
  body: none
  auth: none
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const sessionToken = bru.getEnvVar("sessionToken");
  if (sessionToken) {
    req.setHeader("Cookie", "nadeshiko.session_token=" + sessionToken);
  }
}
