meta {
  name: Retry failed jobs from a queue
  type: http
  seq: 40
}

post {
  url: {{baseUrl}}/v1/admin/queues/:queueName/retry
  body: none
  auth: apikey
}

headers {
  X-API-Key: {{apiKey}}
}

params:query {
  queueName:
}

auth:apikey {
  key: X-API-Key
  value: {{apiKey}}
  placement: header
}

settings {
  encodeUrl: true
  timeout: 0
}
