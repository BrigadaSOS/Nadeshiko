meta {
  name: Get current identity
  type: http
  seq: 6
}

get {
  url: {{baseUrl}}/v1/auth/identity/me
  body: none
  auth: none
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const sessionToken = bru.getEnvVar("sessionToken");
  if (sessionToken) {
    req.setHeader("Cookie", "nadeshiko.session_token=" + sessionToken);
  }
}
