meta {
  name: Get API keys
  type: http
  seq: 8
}

post {
  url: {{baseUrl}}/v1/user/getApiKeys
  body: none
  auth: none
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const sessionToken = bru.getEnvVar("sessionToken");
  if (sessionToken) {
    req.setHeader("Cookie", "nadeshiko.session_token=" + sessionToken);
  }
}
