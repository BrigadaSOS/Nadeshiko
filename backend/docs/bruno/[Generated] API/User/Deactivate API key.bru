meta {
  name: Deactivate API key
  type: http
  seq: 16
}

post {
  url: {{baseUrl}}/v1/user/deactivateApiKey
  body: json
  auth: none
}

body:json {
  {
    "api_key_id": 123
  }
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const jwtToken = bru.getEnvVar("jwtToken");
  if (jwtToken) {
    req.setHeader("Cookie", "access_token=" + jwtToken);
  }
}
