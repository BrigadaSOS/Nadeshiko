meta {
  name: Get user info
  type: http
  seq: 12
}

post {
  url: {{baseUrl}}/v1/jwt/user/info
  body: none
  auth: none
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const jwtToken = bru.getEnvVar("jwtToken");
  if (jwtToken) {
    req.setHeader("Cookie", "access_token=" + jwtToken);
  }
}
