meta {
  name: Get current identity
  type: http
  seq: 13
}

get {
  url: {{baseUrl}}/v1/auth/identity/me
  body: none
  auth: none
}

settings {
  encodeUrl: true
  timeout: 0
}

script:pre-request {
  const jwtToken = bru.getEnvVar("jwtToken");
  if (jwtToken) {
    req.setHeader("Cookie", "access_token=" + jwtToken);
  }
}
