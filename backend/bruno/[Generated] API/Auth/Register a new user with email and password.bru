meta {
  name: Register a new user with email and password
  type: http
  seq: 10
}

post {
  url: {{baseUrl}}/v1/auth/register
  body: json
  auth: inherit
}

headers {
  Referer: {{frontendUrl}}
}

body:json {
  {
    "username": "newuser",
    "email": "newuser@example.com",
    "password": "securepassword123"
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
