meta {
  name: Get Discord URL
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/v1/auth/discord/url
  body: none
  auth: inherit
}

headers {
  x-api-key: {{apiKey}}
}

settings {
  encodeUrl: true
  timeout: 0
}

docs {
  Open the returned URL in your browser to authorize the app.
  After authorization, you'll be redirected with a 'code' parameter.
  Copy that code and use it in the 'Discord Login' request.
}
